{
  "name": "notes-maker",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "git pull origin HEAD && ng serve --proxy-config proxy.config.json --port=4400 --host 0.0.0.0 --disable-host-check",
    "build": "git pull origin HEAD && ng build --prod",
    "build:static": "git add . && git commit -am 'db and other changes' && git push origin HEAD && rm -rf dist && cp -f notesDb/db.json src/assets/db/. && ng build --prod && rm -rf ../static-note-maker/assets && rm -f ../static-note-maker/*.* && cp -r dist/notes-maker/. ../static-note-maker/. && cd ../static-note-maker && git pull origin HEAD && git add . && git commit -am 'changes' && git push && cd ../note-maker",
    "start:api": "json-server --watch notesDb/db.json --routes notesDb/routes.json --port=3001",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.0",
    "@angular/cdk": "^6.2.0",
    "@angular/common": "^6.1.0",
    "@angular/compiler": "^6.1.0",
    "@angular/core": "^6.1.0",
    "@angular/forms": "^6.1.0",
    "@angular/http": "^6.1.0",
    "@angular/material": "^6.4.7",
    "@angular/platform-browser": "^6.1.0",
    "@angular/platform-browser-dynamic": "^6.1.0",
    "@angular/router": "^6.1.0",
    "angular2-promise-buttons": "^3.0.0",
    "core-js": "^2.5.4",
    "ng2-ace-editor": "^0.3.9",
    "rxjs": "~6.2.0",
    "zone.js": "~0.8.26",
    "hammerjs": "2.0.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.8.0",
    "@angular/cli": "~6.2.9",
    "@angular/compiler-cli": "^6.1.0",
    "@angular/language-service": "^6.1.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.3.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~2.9.2"
  }
}
